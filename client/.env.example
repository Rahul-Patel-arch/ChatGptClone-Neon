##########################################
# Example environment variables for Vite #
##########################################

# Gemini / Google Generative AI
VITE_GEMINI_API_KEY=
VITE_GEMINI_API_URL=https://gemini.googleapis.com/v1beta2
VITE_GEMINI_MODEL_NAME=gemini-1.5-flash
VITE_GEMINI_TEMPERATURE=0.7
VITE_GEMINI_TOP_P=0.8
VITE_GEMINI_TOP_K=40
VITE_GEMINI_MAX_TOKENS=2048

# App base URL
VITE_APP_BASE_URL=http://localhost:5173

# OAuth
VITE_GOOGLE_CLIENT_ID=
VITE_GOOGLE_OAUTH_AUTH_URL=https://accounts.google.com/oauth/authorize
VITE_GOOGLE_USERINFO_URL=https://www.googleapis.com/oauth2/v3/userinfo
VITE_MICROSOFT_CLIENT_ID=
VITE_MS_GRAPH_ME_URL=https://graph.microsoft.com/v1.0/me

# EmailJS (client-side demo only)
VITE_EMAILJS_SERVICE_ID=
VITE_EMAILJS_PUBLIC_KEY=
VITE_EMAILJS_WELCOME_TEMPLATE_ID=
VITE_EMAILJS_LOGIN_TEMPLATE_ID=
VITE_EMAILJS_RESET_TEMPLATE_ID=
VITE_SUPPORT_EMAIL=support@quantumchat.local

# Notes:
# - Copy this file to .env and fill in real values locally.
# - Do NOT commit your .env; it's ignored by .gitignore.